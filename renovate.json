{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
      {
        "matchUpdateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "labels": ["minor", "bugfix"],
        "automerge": true,
        "automergeType": "branch"
      }
    ],
    "baseBranches": ["main"]
}
